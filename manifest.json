{
  "manifest_version": 2,
  "name": "Suspicious Quotes",
  "version": "1.0",
  "description": "Randomly adds quotation marks to websites for \"comedic\" effect.",
  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["settings.js", "suspicious-quotes.js"]
  }],
  "browser_action": {
    "default_title": "Suspicious Quotes",
    "default_popup": "popup/popup.html"
  }
}
